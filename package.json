{
  "name": "file-pie",
  "version": "1.0.0",
  "description": "An electron based app that displays the storage used by files and folders in your system in a graphical interactive interface.",
  "main": "src/app/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && electron .",
    "test": "jest specs --runInBand",
    "test:watch": "jest specs --watch --runInBand",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest specs --runInBand",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MajorAchilles/file-pie.git"
  },
  "keywords": [
    "Electron",
    "File",
    "Size",
    "GUI"
  ],
  "author": "Amlanjyoti Saikia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MajorAchilles/file-pie/issues"
  },
  "homepage": "https://github.com/MajorAchilles/file-pie#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-amlan": "0.0.1",
    "eslint-plugin-import": "^2.17.3",
    "jest": "^24.8.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "@material-ui/core": "^4.1.2",
    "@material-ui/icons": "^4.2.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
